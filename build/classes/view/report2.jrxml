	<queryString language="SQL">
		<![CDATA[SELECT 
    o.OrderDate, 
    c.Name AS CustomerName, 
    p.ProductName, 
    p.UnitPrice, 
    o.Quantity, 
    o.TotalValue
FROM 
    orders o
JOIN 
    product p ON o.ProductID = p.ProductID
JOIN 
    customer c ON o.CustomerID = c.CustomerID
ORDER BY 
    o.OrderDate DESC;]]>
	</queryString>
